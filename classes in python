class Car(object):
    def __init__(self, model,color,company,speed_limit):
        self.color = color
        self.company = company
        self.speed_limit = speed_limit
        self.model = model
    def start(self):
        print("started")
    def stop(self):
        print ("stopped")
    
    audi = Car("6", "red", "audi", 80)
    stop(audi)
    start(audi)
